<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B'</ShortCut>
          <Description>Declaration</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Action...">
        <Item>
          <IdeaActionID>GotoAction</IdeaActionID>
          <ShortCut>'⌥A'</ShortCut>
          <Description>Find Action...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⇧⌘F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="New Scratch File">
        <Item>
          <IdeaActionID>NewScratchFile</IdeaActionID>
          <ShortCut>'⇧⌘N'</ShortCut>
          <Description>New Scratch File</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Open Default Scratch">
        <Item>
          <IdeaActionID>Scratch.OpenDefaultScratch</IdeaActionID>
          <ShortCut>'⌥C, ⌥C'</ShortCut>
          <Description>Open Default Scratch</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Open Scratch List">
        <Item>
          <IdeaActionID>Scratch.OpenScratchList</IdeaActionID>
          <ShortCut>'⌥C, ⌥S'</ShortCut>
          <Description>Open Scratch List</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘1'</ShortCut>
          <Description>Project</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Restore Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⇧⌘F12'</ShortCut>
          <Description>Restore Windows</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Select Previous Tab">
        <Item>
          <IdeaActionID>PreviousTab</IdeaActionID>
          <ShortCut>'⇧⌘[' or '⌃←'</ShortCut>
          <Description>Select Previous Tab</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'⌘6'</ShortCut>
          <Description>TODO</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌥F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'⌘9' or '⇧⌘9'</ShortCut>
          <Description>Version Control</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>